import java.util.*;
public class fibonacci 
{
	public static void main(String[] args) 
	{
		Scanner s= new Scanner(System.in);
		System.out.print("Enter the number of elements of series: ");
		int n=s.nextInt();
		int a=0,b=1;
		int i,sum=0;
		for(i=0;i<n;i++)
		{
			if(i==0|| i==1)
			{
				System.out.print(n);
			}
			else
			{
				sum=(i-1)+(i-2);
				System.out.print(sum);
			}
		}
	}
}
